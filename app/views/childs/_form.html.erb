<%= form_for @child, :url => childs_path, :action => 'post', :html => { :multipart => true } do |form| %>

  <%= form.hidden_field :user_id, :value => current_user.id %>

  <%= form.label "Name" %>
  <%= form.text_field :name %>
  <%= form.label "Age" %>
  <%= form.text_field :age %>
  <br>
  <%= form.label "Photo" %>
  <%= form.file_field :avatar %>
  <%= form.submit "send" %>
<% end %>

